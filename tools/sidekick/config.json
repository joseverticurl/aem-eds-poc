{
    "project": "aem-eds-training",
    "plugins": [
      {
        "id": "asset-library",
        "title": "AEM Assets Library",
        "environments": ["edit"],
        "url": "https://experience.adobe.com/solutions/CQ-assets-selectors/static-assets/resources/franklin/asset-selector.html?rail=false",
        "isPalette": true,
        "includePaths": ["**.docx**"],
        "passConfig": true,
        "paletteRect": "top: 50px; bottom: 10px; right: 10px; left: auto; width:600px; height: calc(100vh - 60px)"
      },
      {
        "id": "dynamic-media",
        "title": "Dynamic Media Library",
        "environments": ["edit"],
        "isPalette": true,
        "paletteRect": "top: 50px; bottom: 10px; right: 10px; left: auto; width:600px; height: calc(100vh - 60px)",
        "url": "https://s7sps1.scene7.com/IpsWeb/", 
        "auth": {
          "type": "none"
        },
        "settings": {
          "image": {
            "icon": "icon-image",
            "label": "Dynamic Media Assets"
          },
          "video": {
            "icon": "icon-video",
            "label": "Dynamic Media Videos"
          }
        }
      }
    ]
  }